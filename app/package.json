{
    "name": "app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "prebuild": "npm install esbuild-linux-64",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "jest --verbose --runInBand --detectOpenHandles",
        "updateSnapshots": "jest --updateSnapshot",
        "linting": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
        "prettier": "prettier --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\"",
        "typechecking": "tsc --noEmit",
        "cypress-server": "vite --port 3000",
        "cypress_local": "npx cypress run --browser chrome --env BACKEND_URL=http://localhost:8888",
        "cypress_staging": "npx cypress run --browser chrome  --env BACKEND_URL=https://educado-backend-staging-x7rgvjso4a-ew.a.run.app/"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.398.0",
        "@aws-sdk/lib-storage": "^3.204.0",
        "@dnd-kit/core": "^6.0.5",
        "@dnd-kit/modifiers": "^6.0.0",
        "@dnd-kit/sortable": "^7.0.1",
        "@heroicons/react": "^2.0.11",
        "@hookform/resolvers": "^2.9.8",
        "@mdi/js": "^7.4.47",
        "@mdi/react": "^1.6.1",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.0.0",
        "@tippyjs/react": "^4.2.6",
        "app": "file:",
        "aws-sdk": "^2.1443.0",
        "axios": "^0.27.2",
        "axios-mock-adapter": "^1.22.0",
        "cypress-localstorage-commands": "^2.2.5",
        "daisyui": "^2.38.1",
        "esbuild": "^0.19.12",
        "eslint": "^8.50.0",
        "jwt-decode": "^3.1.2",
        "node": "^22.9.0",
        "prettier": "^3.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.36.1",
        "react-icons": "^5.3.0",
        "react-modal": "^3.16.1",
        "react-pin-field": "^3.1.4",
        "react-player": "^2.11.0",
        "react-quill": "^2.0.0",
        "react-router-dom": "^6.4.1",
        "react-toastify": "^9.0.8",
        "swr": "^1.3.0",
        "usehooks-ts": "^2.9.1",
        "yup": "^0.32.11",
        "zustand": "^4.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.2",
        "@tailwindcss/forms": "^0.5.3",
        "@testing-library/cypress": "^10.0.2",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/jest": "^29.5.8",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/react": "^18.2.21",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@vitejs/plugin-react": "^2.1.0",
        "autoprefixer": "^10.4.12",
        "babel-jest": "^29.7.0",
        "cypress": "^13.14.2",
        "cypress-file-upload": "^5.0.8",
        "jest": "^29.7.0",
        "postcss": "^8.4.16",
        "react-test-renderer": "^18.3.1",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.6.4",
        "vite": "^3.2.7"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
            "\\.(css|less)$": "<rootDir>/mocks/fileMock.js",
            "^@assets(.*)$": "<rootDir>/src/assets$1",
            "^@components(.*)$": "<rootDir>/src/components$1",
            "^@contexts(.*)$": "<rootDir>/src/contexts$1",
            "^@helpers(.*)$": "<rootDir>/src/helpers$1",
            "^@hooks(.*)$": "<rootDir>/src/hooks$1",
            "^@interfaces(.*)$": "<rootDir>/src/interfaces$1",
            "^@pages(.*)$": "<rootDir>/src/pages$1",
            "^@services(.*)$": "<rootDir>/src/services$1",
            "^@utilities(.*)$": "<rootDir>/src/utilities$1"
        },
        "testMatch": [
            "**/?(*.)+(test).[jt]s?(x)"
        ]
    }
}
